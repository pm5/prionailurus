{
  "name": "prionailurus",
  "version": "0.0.1",
  "description": "石虎相關資料",
  "private": true,
  "scripts": {
    "env": "env",
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev:data": "npm run build:data -- -w -v",
    "build:data": "cpx data/**/* build/data",
    "dev:css": "watch 'stylus -m -p -o build/ src/*.styl > build/app.css' src",
    "build:css": "stylus -c -p src/*.styl > build/app.css",
    "dev:image": "npm run build:image -- -w -v",
    "build:image": "cpx 'src/**/*.{png,jpg}' build",
    "dev:html": "npm run build:html -- -w -O '{env:\"dev\"}' -P",
    "build:html": "jade -o build/ src/*.jade",
    "dev:js": "watch 'npm run build:js' src",
    "build:js": "parallelshell 'browserify -o build/app.js src/app.js'",
    "livereload": "live-reload --port 9090 build/",
    "serve": "http-server build/",
    "start": "rimraf build/* && parallelshell 'npm run dev:data' 'npm run dev:html' 'npm run dev:css' 'npm run dev:js' 'npm run dev:image' 'npm run livereload' 'npm run serve'",
    "build": "rimraf build/* && parallelshell 'npm run build:data' 'npm run build:html' 'npm run build:css' 'npm run build:js' 'npm run build:image'",
    "deploy": "npm run build && gh-pages -d build"
  },
  "author": "Pomin Wu <pomin5@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "crossfilter": "^1.3.11",
    "debug": "^2.2.0",
    "jquery": "^2.1.4"
  },
  "devDependencies": {
    "browserify": "^10.2.6",
    "copyfiles": "^0.2.1",
    "cpx": "^1.2.1",
    "d3": "^3.5.6",
    "gh-pages": "^0.4.0",
    "http-server": "^0.7.4",
    "jade": "^1.9.1",
    "live-reload": "^0.2.0",
    "moment": "^2.10.3",
    "parallelshell": "^1.0.3",
    "rimraf": "^2.4.1",
    "stylus": "^0.50.0",
    "topojson": "^1.6.19",
    "watch": "^0.16.0"
  }
}
